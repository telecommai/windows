QWidget
{
background-color: #08172c;
}
QListWidget
{
background-color: #08172c;
border: none;
}
QListWidget::item:selected
{
    border:none;
    background-color: #183257;
	padding: -1px;
}
QScrollBar:vertical
{
	border:none;
    width:12px;
    background:rgba(0,0,0,0%);

}
QScrollBar::handle:vertical
{
    width:12px;
    background:#073552;
    border-radius:4px;  
    min-height:20;
}
QScrollBar::handle:vertical:hover
{
    width:12px;
    background:#073552;   
    border-radius:4px;
    min-height:20;
}
QScrollBar::add-line:vertical   
{
	border:none;
}
QScrollBar::sub-line:vertical  
{
	border:none;
}
QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical   
{
    background: #0a182d;
    border-radius:4px;
}
QWidget#topWidget
{
border-image: url(:/ewallet/Resources/ewallet/assetsBack.png);
}
